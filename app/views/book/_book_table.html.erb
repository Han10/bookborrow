<div id="book_table_container">

	<% @books.each do |book| %>
	<div id="bookcontainer">
		<div id="book_photo">
			<%= image_tag book.thumbnail.url %>
		</div>
		<div id="book_info">
			<div id="book_title">
				<%= book.title %>
			</div>
			<div id="book_desc">
				<%= book.desc book %>
			</div>
			<span id="viewbutton"> View</span>
			<span id="requestbutton"> Request</span>
		</div>
	</div>
	<% end %>
</div>